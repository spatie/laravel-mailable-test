{
    "name": "spatie/laravel-mailable-test",
    "description": "An artisan command to easily test a mailable",
    "keywords": [
        "spatie",
        "laravel-mailable-test"
    ],
    "homepage": "https://github.com/spatie/laravel-mailable-test",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "fzaninotto/faker": "^1.6",
        "illuminate/contracts": "~5.4.0",
        "illuminate/database": "~5.4.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.7",
        "orchestra/database": "~3.4.0@dev",
        "orchestra/testbench": "~3.4.0@dev",
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\MailableTest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\MailableTest\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\MailableTest\\MailableTestServiceProvider"
            ]
        }
    }
}
